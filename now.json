{
  "version": 2,
  "name": "stencil-site",
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "www" } }
  ],
  "routes": [
    { "src": "/sw.js", "headers": { "Cache-Control": "no-cache" } },
    { "src": "/docs/", "status": 301, "headers": { "Location": "/docs/introduction" } },
    { "src": "/docs/building-components", "status": 301, "headers": { "Location": "/docs/my-first-component" } },
    { "src": "/docs/stencil-config", "status": 301, "headers": { "Location": "/docs/config" } },
    { "src": "/docs/testing", "status": 301, "headers": { "Location": "/docs/unit-testing" } },
    { "src": "/docs/shadow-dom", "status": 301, "headers": { "Location": "/docs/styling" } },
    { "src": "/docs/css-variables", "status": 301, "headers": { "Location": "/docs/styling" } },
    { "src": "/docs/sass", "status": 301, "headers": { "Location": "/docs/add-ons" } },
    { "src": "/docs/intro", "status": 301, "headers": { "Location": "/docs/introduction" } },
    { "src": "/docs/templating", "status": 301, "headers": { "Location": "/docs/templating-jsx" } },
    { "src": "/docs/stencil-router", "status": 301, "headers": { "Location": "/docs/router" } },
    { "src": "/docs/routing", "status": 301, "headers": { "Location": "/docs/router" } },
    { "src": "/docs/server-side-rendering", "status": 301, "headers": { "Location": "/docs/prerendering" } },
    { "src": "/docs/add-ons", "status": 301, "headers": { "Location": "/docs/plugins" } },
    { "src": "/docs/build-variables", "status": 301, "headers": { "Location": "/docs/build-conditionals" } }
  ]
}
