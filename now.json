{
	"type": "static",
	"name": "stencil-site",
  "static": {
    "headers": [
			{ "source": "/sw.js", "headers": [{ "key": "cache-control", "value": "no-cache" }]}
		],
    "rewrites": [
      { "source": "**", "destination": "/index.html" }
    ],
    "redirects": [
      { "source": "/docs/", "destination": "/docs/introduction" },
      { "source": "/docs/building-components", "destination": "/docs/my-first-component" },
      { "source": "/docs/stencil-config", "destination": "/docs/config" },
      { "source": "/docs/testing", "destination": "/docs/unit-testing" },
      { "source": "/docs/shadow-dom", "destination": "/docs/styling" },
      { "source": "/docs/css-variables", "destination": "/docs/styling" },
      { "source": "/docs/sass", "destination": "/docs/add-ons" },
      { "source": "/docs/intro", "destination": "/docs/introduction" },
      { "source": "/docs/templating", "destination": "/docs/templating-jsx" },
      { "source": "/docs/stencil-router", "destination": "/docs/router" },
      { "source": "/docs/routing", "destination": "/docs/router" },
      { "source": "/docs/server-side-rendering", "destination": "/docs/prerendering" },
      { "source": "/docs/add-ons", "destination": "/docs/plugins" },
      { "source": "/docs/build-variables", "destination": "/docs/build-conditionals" }
    ]
  }
}
