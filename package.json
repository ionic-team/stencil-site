{
  "name": "@stencil/stencil-site",
  "private": true,
  "version": "0.0.1",
  "description": "Stencil Site",
  "scripts": {
    "build": "npm run site.structure && npm run site.external && npm run docs && stencil build --prerender",
    "build.ci": "npm run site.structure && npm run site.external && npm run docs && stencil build --ci --prerender",
    "docs": "tsc --p ./scripts && node ./scripts/markdown-to-html && node ./scripts/blog-list",
    "prerender": "stencil build --prerender",
    "test": "echo Good",
    "prerender.dev": "stencil build --dev --prerender",
    "site.structure": "tsc --p ./scripts && node ./scripts/site-structure",
    "site.external": "node ./scripts/site-external",
    "start": "stencil build --dev --watch --serve"
  },
  "devDependencies": {
    "@ionic-internal/ionic-ds": "4.1.4",
    "@prismicio/client": "^5.1.0",
    "@stencil/core": "^2.5.2",
    "@stencil/router": "1.0.1",
    "@stencil/state-tunnel": "1.0.1",
    "@types/fs-extra": "^9.0.1",
    "@types/glob": "^7.1.2",
    "@types/marked": "^0.7.4",
    "@types/node": "^14.0.14",
    "@types/prismic-dom": "^2.1.1",
    "@types/prismjs": "^1.16.1",
    "algoliasearch": "^4.3.0",
    "copy-text-to-clipboard": "^2.2.0",
    "dotenv": "^8.2.0",
    "front-matter": "^3.2.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "jest": "^26.1.0",
    "marked": "^0.7.0",
    "node-fetch": "^2.6.1",
    "prismic-dom": "^2.2.3",
    "prismic-javascript": "^2.7.1",
    "prismjs": "1.24.0",
    "rollup-plugin-dotenv": "^0.3.0",
    "typescript": "^3.9.5",
    "workbox-build": "4.3.1"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/stencil-site.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/stencil-site"
  },
  "homepage": "https://stenciljs.com/"
}
